@page "/"
@model ProiectMedii.Pages.Modele.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Clasa_model[0].Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clasa_model[0].Agent.Nume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clasa_model[0].Pret)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clasa_model[0].DataPublicarii)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clasa_model[0].Brand.NumeBrand)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Clasa_model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Model)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Agent.Nume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pret)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataPublicarii)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Brand.NumeBrand)
                </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
